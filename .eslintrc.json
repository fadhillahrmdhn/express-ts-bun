{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "env": {
    "node": true,
    "es2022": true
  },
  "ignorePatterns": [" ** /build/*", " ** /node_modules/*", " ** /public/*"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn"
  }
}
